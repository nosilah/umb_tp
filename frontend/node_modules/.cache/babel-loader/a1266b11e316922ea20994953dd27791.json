A,CAqBO,eArBP,EAqBO;AArBA,IAAA,QAAM,EAAE,QAAA,CAAA,WAqBR;AArBsB,yBAAmB,KAAA,CAAA;AAqBzC,GArBP,E;sBACE;AAAA,aAIQ,CAJR,mBAAA,CAIQ,KAJR,EAAA,UAAA,EAIQ,CAHJ,UAGI,EAFJ,YAAA,CAAwD,gBAAxD,EAAwD;AAAjD,QAAA,IAAI,EAAC,OAA4C;AAApC,QAAA,IAAI,EAAC,OAA+B;AAAvB,QAAA,KAAK,EAAC;AAAiB,OAAxD,CAEI,EADJ,YAAA,CAAoD,uBAApD,EAAoD;AAAtC,QAAA,IAAI,EAAC,OAAiC;AAAzB,QAAA,KAAK,EAAC;AAAmB,OAApD,CACI,CAJR,CAIQ,EACR,mBAAA,CAQM,KARN,EAAA,UAAA,EAQM,CAPJ,mBAAA,CAMS,QANT,EAMS;AAND,QAAA,KAAK,EAAC,2BAML;AANkC,QAAA,QAAQ,EAAE,KAAA,CAAA;AAM5C,OANT,E,iBACE,mBAAA,CAGQ,MAHR,EAAA,UAAA,EAGQ,IAHR,EAGQ,GAHR,C,EAGQ,C,SAFE,KAAA,CAAA,O,CAEF,C,GACR,U,CALF,E,CAAA,E,UAAA,CAOI,CARN,CADQ,EAWR,mBAAA,CAIM,KAJN,EAAA,WAAA,EAIM,CAHO,KAAA,CAAA,O,kBAAX,mBAAA,CAEM,KAFN,EAAA,WAAA,EAEM,gBAAA,CADD,KAAA,CAAA,OACC,CAFN,EACY,CADZ,C,iCAGI,CAJN,CAXQ,CAJR;AAAA,K;;GADF,E,CAAA,E,iCAAA,CAsBI,CA5BN,CA6BI,CA9BN,C","sourcesContent":["<template>\n  <div class=\"col-md-12\">\n    <div class=\"card card-container\">\n      <img\n        id=\"profile-img\"\n        src=\"//ssl.gstatic.com/accounts/ui/avatar_2x.png\"\n        class=\"profile-img-card\"\n      />\n      <Form @submit=\"handleLogin\" :validation-schema=\"schema\">\n        <div class=\"form-group\">\n            <label for=\"email\">Email</label>\n            <Field name=\"email\" type=\"email\" class=\"form-control\" />\n            <ErrorMessage name=\"email\" class=\"error-feedback\" />\n          </div>\n        <div class=\"form-group\">\n          <button class=\"btn btn-primary btn-block\" :disabled=\"loading\">\n            <span\n              v-show=\"loading\"\n              class=\"spinner-border spinner-border-sm\"\n            ></span>\n            <span>Login</span>\n          </button>\n        </div>\n\n        <div class=\"form-group\">\n          <div v-if=\"message\" class=\"alert alert-danger\" role=\"alert\">\n            {{ message }}\n          </div>\n        </div>\n      </Form>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { Form, Field, ErrorMessage } from \"vee-validate\";\nimport * as yup from \"yup\";\n\nexport default {\n  name: \"Login\",\n  components: {\n    Form,\n    Field,\n    ErrorMessage,\n  },\n  data() {\n    const schema = yup.object().shape({\n       email: yup\n        .string()\n        .required(\"Enter the email address!\")\n        .email(\"Email is invalid!\")\n        .max(50, \"Must be maximum 50 characters!\"),\n      \n    });\n\n    return {\n      loading: false,\n      message: \"\",\n      schema,\n\n    };\n  },\n  computed: {\n    loggedIn() {\n      this.message = 'check you email'\n      return this.$store.state.auth.status.loggedIn;\n    },\n  },\n  created() {\n    if (this.loggedIn) {\n      this.$router.push(\"/profile\");\n    }\n  },\n  methods: {\n    handleLogin(user) {\n      this.loading = true;\n\n      this.$store.dispatch(\"auth/login\", user).then(\n        () => {\n          // this.$router.push(\"/profile\");\n          this.$router.push({name: 'message'});\n          console.log('it is login');\n        },\n        (error) => {\n          this.loading = false;\n          this.message =\n            (error.response &&\n              error.response.data &&\n              error.response.data.message) ||\n            error.message ||\n            error.toString();\n        }\n      );\n    },\n\n   \n\n  },\n};\n</script>\n\n<style scoped>\nlabel {\n  display: block;\n  margin-top: 10px;\n}\n\n.card-container.card {\n  max-width: 350px !important;\n  padding: 40px 40px;\n}\n\n.card {\n  background-color: #f7f7f7;\n  padding: 20px 25px 30px;\n  margin: 0 auto 25px;\n  margin-top: 50px;\n  -moz-border-radius: 2px;\n  -webkit-border-radius: 2px;\n  border-radius: 2px;\n  -moz-box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.3);\n  -webkit-box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.3);\n  box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.3);\n}\n\n.profile-img-card {\n  width: 96px;\n  height: 96px;\n  margin: 0 auto 10px;\n  display: block;\n  -moz-border-radius: 50%;\n  -webkit-border-radius: 50%;\n  border-radius: 50%;\n}\n\n.error-feedback {\n  color: red;\n}\n</style>\n"],"sourceRoot":""}]}